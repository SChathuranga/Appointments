<html>

<head>
	<title>eChanneling - Sign Up</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<!-- load bootstrap css -->
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">

	<link rel="stylesheet" href="/assets/css/main.css" />
	<!-- load fontawesome -->
	<style>
		body {
			padding-top: 80px;
		}
	</style>
</head>

<body>
	<!-- Header -->
	<header id="header">
		<a class="logo" href="home">eChanneling</a>
		<nav>
			<a href="#menu">Menu</a>
		</nav>
	</header>
	<!-- Banner -->
	<section id="banner">
		<div class="inner">
			<h2>
				Institute Edit
			</h2>

			<div class="col-md-3"></div>
			<div class="col-md-6 limiter">
				<div class="wrap-login100">
					<!-- LOGIN FORM -->
					<form action="/instituteupdate/<%= id %>" method="post">
						<div class="form-group">
							Name
							<input type="text" class="form-control" name="institutename" id="name" value="<%= institutename %>" required/>
						</div>
						<div class="form-group">
							Address
							<input type="text" class="form-control" name="address" value="<%= address %>"  required/>
						</div>

						<div class="form-group">
							Contact
							<input type="text" class="form-control" name="contact" value="<%= contact %>" required/>
						</div>

						<input type="Submit" class="btn btn-warning btn-lg" name="Submit" value="Edit" required/>
					</form>
				</div>
			</div>
			<div class="col-md-3"></div>
		</div>
	</section>
</body>

</html>
